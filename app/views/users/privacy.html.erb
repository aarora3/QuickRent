<h1>User Privacy Setting</h1>
<p><%= @user.name %></p>
<p id="notice"><%= notice %></p>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      </br>
      <%= f.label :email %>
      <%= f.email_field :email %>
      </br>  
      <%= f.label :password %>
      <%= f.password_field :password %>
      </br>
      <%= f.label :passwordconfirmation %>
      <%= f.password_field :passwordconfirmation %>
      </br>
      <%= f.label :contactnumber %>
      <%= f.text_field :contactnumber %>
      </br>
      <%= f.label :house_address %>
      <%= f.text_field :house_address %>
      </br>
      <%= f.label :city %>
      <%= f.text_field :city %>
      </br><br>
      <%= f.submit "Submit", class: "btn btn-primary" %>
      </br></br>
    <% end %>
  </div>
</div>
<%= link_to 'Back to My Account', @user %>